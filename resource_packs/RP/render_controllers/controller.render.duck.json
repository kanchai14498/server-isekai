{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.duck": {
			"arrays": {
				"textures": {
					"Array.skins": [
						"Texture.default",
						"Texture.duck_two"
					]
				}
			},
			"geometry": "geometry.default",
