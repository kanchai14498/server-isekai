{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.unbound_elemental": {
			"geometry": "geometry.default",
			"materials": [
				{
					"*": "material.default"
				},
				{
					"*": "material.glow"
				},
				{
					"*": "material.glow2"
				}
			],
			"textures": [
				"texture.default"
			],
			"part_visibility": [
				{
					"helmet": "q.health >= 200 && !query.is_saddled"
				}
			]
		}
	}
}