{
	"format_version": "1.8.0",
	"animations": {
		"animation.armadillo_projectile.horizontal": {
			"loop": true,
			"bones": {
				"root": {
					"rotation": [0, 0, 90]
				}
			}
		},
		"animation.armadillo_projectile.rolling": {
			"loop": true,
			"bones": {
				"body": {
					"rotation": ["variable.rolling * 720", 0, 0],
					"position": [0, "math.sin(variable.rolling * 360) * 2", "math.cos(variable.rolling * 360) * 2"]
				}
			}
		}
	}
}