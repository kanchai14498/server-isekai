			"is_summonable": true,
			"is_experimental": false,
			"scripts": {
				"animate": [
					{
						"no_target": "!query.has_target"
					},
					{
						"target": "query.has_target&&q.property('dk:modes')=='standing'"
					},
					{
						"target_shield": "query.has_target&&q.property('dk:modes')=='standing'&&q.property('dk:weapons')=='sword'&&q.property('dk:states')=='melee'&&q.property('dk:shield_cooldown')=='no_cooldown'"
					},
					{
						"home": "query.is_saddled&&q.property('dk:target')=='no_target'&&q.property('dk:patrol')=='no_patrol'&&!query.is_tamed"
					},
					{
						"food": "query.is_tamed&&q.property('dk:target')=='no_target'"
					},
					{
						"weapons": "query.has_target&&q.property('dk:modes')=='standing'"
					},
					{
						"despawn": "q.property('dk:patrol')=='patrol'"
					},
					"potion",
					"drink"
				]
			},
